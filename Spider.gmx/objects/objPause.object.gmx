          <argument>
            <kind>1</kind>
            <string>/// Pause Game

// pause game if not already paused
if(keyboard_check_pressed(vk_space)){
    if(global.pause == 0){
        global.pause = 1;
        physics_pause_enable(true);
    } 
}

// game paused
if(global.pause){
    with(objPlayer){
        image_speed = 0;
    }
    
    // left mouse button pressed
    if(mouse_check_button_pressed(mb_left)){
        
        var mx = mouse_x - view_xview[0];
        var my = mouse_y - view_yview[0];
        
        if(mx &gt;= 437 &amp;&amp; my &lt;= 609){
            global.pause = 0;
            
            // resume game
            if(my &gt;= 159 &amp;&amp; my &lt;= 194){
                physics_pause_enable(false);
            } 
            
            // restart level
            else if(my &gt;= 210 &amp;&amp; my &lt;= 249){
                room_restart();
                instance_destroy();
            } 
            
            // go to main menu
            else if(my &gt;= 268 &amp;&amp; my &lt;= 302){
                room_goto(rmStartMenu);
                instance_destroy();
            } 
            
            // exit game
            else if(my &gt;= 324 &amp;&amp; my &lt;= 360){
                game_end();
            }
        }
    }
}
</string>
          </argument>
        </arguments>
      </action>
