            <kind>1</kind>
            <string>/// Pause Game

if (keyboard_check_pressed(vk_space)) {
    if (global.pause == 0) {
        global.pause = 1;
        physics_pause_enable(true);
    }
}

if(global.pause){
    with(objPlayer){
        image_speed = 0;
    }
    
    if(mouse_check_button_pressed(mb_left)){
        var myx = mouse_x - view_xview[0];
        var myy = mouse_y - view_yview[0];
        
        
        if(myx &gt;= 534 &amp;&amp; myx &lt;= 684){
        
            // resume game
            if (myy &gt;= 134 &amp;&amp; myy &lt;= 165) {
                global.pause = 0;
                physics_pause_enable(false);
            } 
            
            // restart room
            else if (myy &gt;= 182 &amp;&amp; myy &lt;= 217) {
                room_restart();
            }
            
            // main menu
            else if (myy &gt;= 237 &amp;&amp; myy &lt;= 270) {}
            
            // exit game
            else if (myy &gt;= 291 &amp;&amp; myy &lt;= 329) {
                game_end();
            }
        }
    }
}
</string>
